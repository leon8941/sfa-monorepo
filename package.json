{
  "name": "sfa-monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "db-migrate": "yarn exec prisma migrate dev --schema=./packages/databases/backoffice-db/src/prisma/schema.prisma --name",
    "db-studio": "yarn exec prisma studio --schema=./packages/databases/backoffice-db/src/prisma/schema.prisma",
    "db-g": "yarn exec prisma generate --schema=./packages/databases/backoffice-db/src/prisma/schema.prisma",
    "format": "yarn exec nx format",
    "serve-api": "yarn db-g && yarn exec nx serve backoffice-api",
    "serve-backoffice": "yarn db-g && yarn exec nx serve backoffice-apps"
  },
